{
	"name": "TWOverflow - Tribal Wars 2",
	"version": "2.0.0",
	"description": "Script Bundle for Tribal Wars 2",
	"permissions": [
        "https://*.tribalwars2.com/game.php*"
    ],
	"icons": {
		"128": "overflow.png"
	},
    "browser_action": {
        "default_icon": "overflow.png"
    },
	"content_scripts": [{
		"js": ["loader.js"],
		"matches": ["https://*.tribalwars2.com/game.php*"],
		"run_at": "document_end"
	}],
	"manifest_version": 2
}